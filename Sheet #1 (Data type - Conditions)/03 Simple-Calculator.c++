#include <iostream>
using namespace std;

int main() {
    long long num1, num2;

    // Input
    cin >> num1 >> num2;

    // Output: Addition, Multiplication, Subtraction
    cout << num1 << " + " << num2 << " = " << num1 + num2 << endl;
    cout << num1 << " * " << num2 << " = " << num1 * num2 << endl;
    cout << num1 << " - " << num2 << " = " << num1 - num2 << endl;

    return 0;
}
